{
  "name": "my-budget",
  "version": "1.0.0",
  "description": "my budget",
  "scripts": {
    "start:2": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc --rootDir . --outDir dist",
    "tsc:w": "tsc -w --rootDir . --outDir dist",
    "tsc:s": "tsc --removeComments --module commonjs --target ES5 --outDir dist/server server/server.ts",
    "typings": "typings",
    "fix-express": "typings install dt~express dt~serve-static dt~express-serve-static-core dt~mime --global",
    "webdriver:update": "webdriver-manager update"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
